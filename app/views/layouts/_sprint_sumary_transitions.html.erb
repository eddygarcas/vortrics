

<div class="col-lg-3 col-md-6 col-sm-12 animate-drop-fade">
  <div class="panel panel-warning">
    <div class="panel-heading">
      <p class="lead"><%= fa_icon_tag("wrench") %><%= puts_non_nill(@team.longer_issue, :key) %> <b>In Progress</b></p>
    </div>
    <div class='panel-body text-orange-400 text-center'>
      <h2 class="m0"><%= puts_no_defined(@team.longer_issue.time_in({toString: :wip},{toString: :testing})) %></h2>
      <small>Time</small>

    </div>
    <div class="panel-footer">
      <div class="row">
        <div class="col-sm-12">
          <%= vt_work_time_tag puts_non_nill(@team.shortest_issue, :key), ' In Progress', puts_no_defined(@team.shortest_issue.time_in({toString: :wip},{toString: :testing})) %>
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>


