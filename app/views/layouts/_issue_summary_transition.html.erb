<div class="col-lg-3 col-md-6 col-sm-12 animate-drop-fade">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <p class="lead"><i class="fa fa-list"></i><%= puts_non_nill(@issue, :key) %> <b> Open</b></p>
    </div>
    <div class='panel-body text-blue-400 text-center'>
      <h2 class='page-hits m0'><%= puts_no_defined(@issue.time_in :open, :wip) %></h2>
      <small>Time</small>
    </div>
    <div class="panel-footer">
      <div class="row">
        <div class="col-sm-12">
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>
<div class="col-lg-3 col-md-6 col-sm-12 animate-drop-fade">
  <div class="panel panel-primary">
    <div class="panel-heading">
      <p class="lead"><%= fa_icon_tag("wrench") %><%= puts_non_nill(@issue, :key) %> <b> In Progress</b></p>
    </div>
    <div class='panel-body text-light-blue-400 text-center'>
      <h2 class="m0"><%= puts_no_defined(@issue.time_in :wip, :testing) %></h2>
      <small>Time</small>

    </div>
    <div class="panel-footer">
      <div class="row">
        <div class="col-sm-12">
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>
