<div class="panel panel-info">
  <div class="panel-heading">
    <p class="lead"><%= fa_icon_tag("clock-o") %> Through<b>put</b></p>
  </div>
  <div class='panel-body text-blue-400 text-center' data-toggle="popover" data-placement="bottom" data-content="<%= ScrumMetrics.config[:popover][:varianza] %>" data-original-title="" title="" id="popoverVarianza">
    <h2 class="m0"><%= team.sprint.throughput %></h2>
    <small>Items daily</small>
  </div>

  <div class="panel-footer">
    <div class="row">
      <div class="col-sm-12">
        As result of <strong><%= team.sprint.stories + team.sprint.bugs%></strong> completed items on <strong><%= team.sprint.days %></strong> days
      </div>
    </div>
    <div class="clearfix"></div>
  </div>
</div>